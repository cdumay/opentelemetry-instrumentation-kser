{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "group:all"
  ],
  "packageRules": [
    {
      "ignoreUnstable": false,
      "matchPaths": [
        "*"
      ]
    }
  ],
  "separateMajorMinor": false,
  "separateMinorPatch": false,
  "minimumReleaseAge": "3 days",
  "dependencyDashboard": false
}
